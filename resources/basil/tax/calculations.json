{
    "methods": {
        "create": {
            "arguments": {
                "currency": "iso_currency_code",
                "line_items": {
                    "amount": "currency",
                    "quantity": "currency"
                },
                "shipping_cost": {
                    "amount": "currency"
                },
                "tax_date": "timestamp"
            },
            "httpMethod": "post",
            "path": "/v1/tax/calculations"
        },
        "listLineItems": {
            "path": "/v1/tax/calculations/{calculation_id}/line_items"
        },
        "retrieve": {
            "path": "/v1/tax/calculations/{calculation_id}"
        }
    }
}
