{
    "methods": {
        "create": {
            "arguments": {
                "fixed_amount": {
                    "amount": "currency",
                    "currency": "iso_currency_code"
                }
            },
            "httpMethod": "post",
            "path": "/v1/shipping_rates"
        },
        "list": {
            "arguments": {
                "active": "boolean",
                "created": "datefilter"
            },
            "path": "/v1/shipping_rates"
        },
        "retrieve": {
            "path": "/v1/shipping_rates/{shipping_rate_id}"
        },
        "update": {
            "arguments": {
                "active": "boolean"
            },
            "httpMethod": "post",
            "path": "/v1/shipping_rates/{shipping_rate_id}"
        }
    }
}
