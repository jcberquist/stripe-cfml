{
    "methods": {
        "cancelAction": {
            "httpMethod": "post",
            "path": "/v1/terminal/readers/{reader_id}/cancel_action"
        },
        "create": {
            "httpMethod": "post",
            "path": "/v1/terminal/readers"
        },
        "delete": {
            "httpMethod": "delete",
            "path": "/v1/terminal/readers/{reader_id}"
        },
        "list": {
            "path": "/v1/terminal/readers"
        },
        "processPaymentIntent": {
            "arguments": {
                "process_config": {
                    "enable_customer_cancellation": "boolean",
                    "skip_tipping": "boolean",
                    "tipping": {
                        "amount_eligible": "currency"
                    }
                }
            },
            "httpMethod": "post",
            "path": "/v1/terminal/readers/{reader_id}/process_payment_intent"
        },
        "processSetupIntent": {
            "arguments": {
                "process_config": {
                    "enable_customer_cancellation": "boolean"
                }
            },
            "httpMethod": "post",
            "path": "/v1/terminal/readers/{reader_id}/process_setup_intent"
        },
        "refundPayment": {
            "arguments": {
                "amount": "currency",
                "refund_application_fee": "boolean",
                "refund_payment_config": {
                    "enable_customer_cancellation": "boolean"
                },
                "reverse_transfer": "boolean"
            },
            "httpMethod": "post",
            "path": "/v1/terminal/readers/{reader_id}/refund_payment"
        },
        "retrieve": {
            "path": "/v1/terminal/readers/{reader_id}"
        },
        "setReaderDisplay": {
            "arguments": {
                "cart": {
                    "currency": "iso_currency_code",
                    "line_items": {
                        "amount": "currency"
                    },
                    "tax": "currency",
                    "total": "currency"
                }
            },
            "httpMethod": "post",
            "path": "/v1/terminal/readers/{reader_id}/set_reader_display"
        },
        "update": {
            "httpMethod": "post",
            "path": "/v1/terminal/readers/{reader_id}"
        }
    }
}
