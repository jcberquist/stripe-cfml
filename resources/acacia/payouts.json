{
    "methods": {
        "cancel": {
            "httpMethod": "post",
            "path": "/v1/payouts/{payout_id}/cancel"
        },
        "create": {
            "arguments": {
                "amount": "currency",
                "currency": "iso_currency_code"
            },
            "httpMethod": "post",
            "path": "/v1/payouts"
        },
        "list": {
            "arguments": {
                "arrival_date": "datefilter",
                "created": "datefilter"
            },
            "path": "/v1/payouts"
        },
        "retrieve": {
            "path": "/v1/payouts/{payout_id}"
        },
        "reverse": {
            "httpMethod": "post",
            "path": "/v1/payouts/{payout_id}/reverse"
        },
        "update": {
            "httpMethod": "post",
            "path": "/v1/payouts/{payout_id}"
        }
    }
}
