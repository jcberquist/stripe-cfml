{
    "methods": {
        "close": {
            "httpMethod": "post",
            "path": "/v1/disputes/{dispute_id}/close"
        },
        "list": {
            "arguments": {
                "created": "datefilter"
            },
            "path": "/v1/disputes"
        },
        "retrieve": {
            "path": "/v1/disputes/{dispute_id}"
        },
        "update": {
            "arguments": {
                "submit": "boolean"
            },
            "httpMethod": "post",
            "path": "/v1/disputes/{dispute_id}"
        }
    }
}
