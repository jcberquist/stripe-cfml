{
    "methods": {
        "create": {
            "arguments": {
                "individual": {
                    "card_issuing": {
                        "user_terms_acceptance": {
                            "date": "timestamp"
                        }
                    }
                },
                "spending_controls": {
                    "spending_limits": {
                        "amount": "currency"
                    }
                }
            },
            "httpMethod": "post",
            "path": "/v1/issuing/cardholders"
        },
        "list": {
            "arguments": {
                "created": "datefilter"
            },
            "path": "/v1/issuing/cardholders"
        },
        "retrieve": {
            "path": "/v1/issuing/cardholders/{cardholder_id}"
        },
        "update": {
            "arguments": {
                "individual": {
                    "card_issuing": {
                        "user_terms_acceptance": {
                            "date": "timestamp"
                        }
                    }
                },
                "spending_controls": {
                    "spending_limits": {
                        "amount": "currency"
                    }
                }
            },
            "httpMethod": "post",
            "path": "/v1/issuing/cardholders/{cardholder_id}"
        }
    }
}
