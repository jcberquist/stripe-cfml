{
    "methods": {
        "cancel": {
            "httpMethod": "post",
            "path": "/v1/treasury/inbound_transfers/{inbound_transfer_id}/cancel"
        },
        "create": {
            "arguments": {
                "amount": "currency",
                "currency": "iso_currency_code"
            },
            "httpMethod": "post",
            "path": "/v1/treasury/inbound_transfers"
        },
        "list": {
            "path": "/v1/treasury/inbound_transfers"
        },
        "retrieve": {
            "path": "/v1/treasury/inbound_transfers/{inbound_transfer_id}"
        }
    }
}
